local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "spooky.pub", HidePremium = false, IntroEnabled = false, IntroText = "spooky.pub", SaveConfig = true, ConfigFolder = "spooky.pub"})

-- Values
_G.LongNeck = true


-- Functions



local CombatTab = Window:MakeTab({
	Name = "Combat",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
 
local HitboxSection = CombatTab:AddSection({
	Name = "Hitbox"
})

local MiscTab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false

local LongNeckSection = CombatTab:AddSection({
	Name = "LongNeck"
})

