local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
 
local Window = OrionLib:MakeWindow({Name = "spooky.pub", HidePremium = false, SaveConfig = true, ConfigFolder = "spooky.pub"})
 
local CombatTab = Window:MakeTab({
	Name = "Combat",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
 
local HitboxSection = CombatTab:AddSection({
	Name = "Hitbox"
})

local LongNeckSection = CombatTab:AddSection({
	Name = "LongNeck"
})
